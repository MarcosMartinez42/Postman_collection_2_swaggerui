		{
			"name": "Parte_2",
			"item": [
				{
					"name": "Obtener mascotas disponibles y crear variable global con array",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"let mascotas_disponibles = pm.response.json();\r",
									"let mascotasSeleccionadas = mascotas_disponibles.slice(0, 5);\r",
									"pm.globals.set(\"arraymascotas\", JSON.stringify(mascotasSeleccionadas));"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{Base_URL}}/pet/findByStatus?status=available",
							"host": [
								"{{Base_URL}}"
							],
							"path": [
								"pet",
								"findByStatus"
							],
							"query": [
								{
									"key": "status",
									"value": "available"
								}
							]
						}
					},
					"response": []
				},
				{
					"name": "Crear órdenes para las 5 mascotas guardadas",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"let arraymascotas = JSON.parse(pm.globals.get(\"arraymascotas\"));\r",
									"let ordenes = []\r",
									"if (arraymascotas.length > 0) {\r",
									"       for (let i = 0; i < arraymascotas.length; i++) {\r",
									"        let unaMascota = arraymascotas[i];\r",
									"        let orden = {\r",
									"            id: i+1,\r",
									"            petId: unaMascota.id,\r",
									"            quantity: 1,\r",
									"            shipDate: new Date().toISOString(),\r",
									"            status: \"placed\",\r",
									"            complete: true\r",
									"    }\r",
									"    ordenes.push(orden)\r",
									"}\r",
									" ordenes.forEach((orden) => {\r",
									"        pm.sendRequest({\r",
									"            url: \"https://petstore.swagger.io/v2/store/order\",\r",
									"            method: \"POST\",\r",
									"            header: {\r",
									"                \"Content-Type\": \"application/json\"\r",
									"            },\r",
									"            body: {\r",
									"                mode: \"raw\",\r",
									"                raw: JSON.stringify(orden)\r",
									"            }\r",
									"        }, function (err, res) {\r",
									"            if (err) {\r",
									"                console.log(\"La orden no pudo ser creada:\", err);\r",
									"            } else {\r",
									"                console.log(\"Orden creada de manera exitosa:\", res.json());\r",
									"            }\r",
									"        });\r",
									"    });\r",
									"}"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\r\n  \"id\": 0,\r\n  \"petId\": 1,\r\n  \"quantity\": 0,\r\n  \"shipDate\": \"2024-10-24T04:10:09.158Z\",\r\n  \"status\": \"placed\",\r\n  \"complete\": true\r\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{Base_URL}}/store/order",
							"host": [
								"{{Base_URL}}"
							],
							"path": [
								"store",
								"order"
							]
						}
					},
					"response": []
				}
			]
